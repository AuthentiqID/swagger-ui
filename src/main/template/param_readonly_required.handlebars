<td class='code required'>
  <label for='{{{escape valueId}}}'>{{{sanitize name}}}</label>
  <span class="glyphicon glyphicon glyphicon-asterisk text-primary" aria-hidden="true"></span>
</td>
<td>
    {{#if isBody}}
        <textarea class='body-textarea form-control' readonly='readonly' placeholder='(required)' name='{{{sanitize name}}}' id='{{{escape valueId}}}'>{{{sanitize default}}}</textarea>
    {{else}}
        {{#if default}}
            {{{sanitize default}}}
        {{else}}
            (empty)
        {{/if}}
    {{/if}}
</td>
<td class="markdown param-description-col">{{{sanitize description}}}</td>
<td>{{{escape paramType}}}</td>
<td><span class="model-signature"></span></td>
